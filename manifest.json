{
  "manifest_version": 3,  
  "name": "ProjectV-Tracker",
  "version": "0.1",
  "description": "It shows the ranks of all participating players in a Match.",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "browser_action": {
    "default_popup": "popup.html"
  }
}